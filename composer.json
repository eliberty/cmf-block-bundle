{
    "name": "eliberty/cmf-block-bundle",
    "type": "symfony-bundle",
    "description": "Symfony CMF Block Bundle",
    "keywords": [
        "Symfony CMF",
        "Block",
        "content fragments"
    ],
    "homepage": "http://cmf.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Symfony CMF Community",
            "homepage": "https://github.com/symfony-cmf/BlockBundle/contributors"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://e-liberty.repo.repman.rpstack.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/framework-bundle": "^4.0",
        "doctrine/phpcr-bundle": "^2.5",
        "doctrine/phpcr-odm": "^1.4 || ^2.0",
        "sonata-project/block-bundle": "^3.11 || ^4",
        "eliberty/cmf-core-bundle": "^2.500"
    },
    "require-dev": {
        "symfony-cmf/testing": "^2.1.8",
        "eliberty/cmf-menu-bundle": "^2.500",
        "symfony/phpunit-bridge": "^4.0 || ^5.0",
        "twig/twig": "^2.0 || ^3.0",
        "symfony/security-bundle": "^2.8 || ^3.3 || ^4.0",
        "symfony/twig-bundle": "^4.0 || ^5.0",
        "symfony/asset": "^4.0 || ^5.0",
        "symfony/templating": "^4.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "rector/rector": "^0.15.21"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "conflict": {
        "sonata/cache": "<1.1.1"
    }
}
